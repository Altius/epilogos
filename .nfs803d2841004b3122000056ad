    [38;5;130mfor[m biosample [38;5;130min[m [35m$(cut -f1 ${metadata} | [m[38;5;130mtail[m[35m -n +2)[m; [38;5;130mdo[42;7Hif compgen[m [38;5;130m-G[m [38;5;130m"[m[35m${datadir}[m[31m/*[m[35m${biosample}[m[31m*[m[35m${chr}[m[31m_*.txt*[m[38;5;130m"[m [38;5;130m>[m /dev/null; [38;5;130mthen[m[43;9H[36mnam[m=[35m`stat -t -c %n ${datadir}/*${biosample}*${chr}_*.txt*`[m[44;9H[36mcmd[m=[38;5;130m"[m[35m$cmd[m[31m <(zcat -f -- [m[38;5;130m"[m[35m${nam}[m[38;5;130m"[m[31m)[m[38;5;130m"[m;[45;9H[36mnumfiles_actual+[m=[31m1[m[46;7H[38;5;130mfi
