[m  [38;5;130mfi
done[m[57;1H[94m~                                                                                                                                                                                                                   [m[58;195H41,7[10CAll[41;7H[?12l[?25h[?25l[58;198H8[41;8H[?12l[?25h[?25l[58;198H9[41;9H[?12l[?25h[?25l[1C[38;5;130m[106m[[45C][m[58;198H10[41;10H[?12l[?25h[?25l[58;1H[1m-- INSERT --[m[58;13H[K[58;195H41,11[9CAll[58;195H[K[58;195H41,11[9CAll[41;11H[?12l[?25h[?25l[35m[106m[[m[35m[[m [38;5;130m-e[m [38;5;130m"[m[35m${datadir}[m[31m/*[m[35m${biosample}[m[31m*[m[35m${chr}[m[31m_*.txt*[m[38;5;130m"[m[106m [m][38;5;130m;[m then[42;9Hnam[38;5;130m=[m[43;9Hecho [44;9Hcmd[38;5;130m=[m[31m"$cmd[14C"[m[9C[38;5;130m;[m[45;7Hfi
