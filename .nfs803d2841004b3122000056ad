[?1049h[?1h=[2;1H▽[6n[1;1H  [1;1H[1;58r[?12;25h[?12l[?25h[27m[m[H[2J[?25l[58;1H"scripts/preprocess_data_ChromHMM.sh" 55L, 2184C[>c[1;1H[34m#!/bin/bash[m
[38;5;130mset[m[36m [m[35m-ueo[m[36m pipefail[m

[38;5;130mif [[m [35m$#[m [38;5;130m-lt[m [31m3[m [38;5;130m][m; [38;5;130mthen
[m  [38;5;130mecho
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31mUsage: [m[35m$0[m[31m <chromsizes> <metadata> <datadir>[m[38;5;130m"
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31mInsufficient information -- please provide:[m[38;5;130m"
[m  [38;5;130mecho
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31m1. <datadir>: Directory with input data, one file per biosample-chromosome combination.[m[38;5;130m"
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31m   These can be obtained from ChromHMM using the '-printstatebyline' parameter.[m[38;5;130m"
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31m   Each line expected to represent a single 200bp genomic bin and its chromatin state.[m[38;5;130m"
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31m   For more information on the formatting of these files, see the ChromHMM manual at:[m[38;5;130m"
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31m     http://compbio.mit.edu/ChromHMM/ChromHMM_manual.pdf[m[38;5;130m"
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31m   Example files can be found in the 'data/ChromHMM' directory.[m[38;5;130m"
[m  [38;5;130mecho
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31m2. <metadata>: File containing metadata information for each input dataset/biosample.[m[38;5;130m"
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31m   This file is expected to contain a header line, and a unique biosample name in column 1,[m[38;5;130m"
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31m   represented in the input data directory. Example file: 'data/metadata_Boix.txt'.[m[38;5;130m"
[m  [38;5;130mecho
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31m3. <chromsizes>: File containing chromosome sizes in number of basepairs.[m[38;5;130m"
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31m   This information can be obtained from UCSC, e.g. for human genome hg19, run:[m[38;5;130m"
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31m   $ wget -q -O - ftp://hgdownload.cse.ucsc.edu/goldenPath/hg19/database/chromInfo.txt.gz | [m[35m\\[m[38;5;130m"
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31m       gunzip - | cut -f 1-2 > hg19.genome[m[38;5;130m"
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31m   Examples for hg19 and hg38 can be found in the 'scripts' directory.[m[38;5;130m"
[m  [38;5;130mecho
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31mExample run: [m[35m$0[m[31m data/ChromHMM data/metadata_Boix.txt scripts/hg19.genome[m[38;5;130m"
[m  [38;5;130mexit
fi[m

[36mdatadir[m=[35m${1}[m
[36mmetadata[m=[35m${2}[m
[36mchromsizes[m=[35m${3}[m

[38;5;130mfor[m chr [38;5;130min[m [35m$(cut -f1 ${chromsizes})[m; [38;5;130mdo
[m  [38;5;130mecho[m[31m -n [m[38;5;130m"[m[31mProcessing [m[35m${chr}[m[31m: [m[38;5;130m"[m
  [36mcmd[m=[38;5;130m"[m[31mpaste[m[38;5;130m"[m;
  [36mnumfiles[m=[35m`[m[38;5;130mls[m[35m [m[31m-1[m[35m ${datadir}/*${chr}_*.txt* [m[31m2[m[35m>/dev/null | wc -l`[m
  [38;5;130mif [[m [35m${numfiles}[m [38;5;130m-gt[m [31m0[m [38;5;130m][m; [38;5;130mthen
[m    [38;5;130mecho[m[31m [m[38;5;130m"[m[35m${numfiles}[m[31m files[m[38;5;130m"
[m    [38;5;130mfor[m biosample [38;5;130min[m [35m$(cut -f1 ${metadata} | [m[38;5;130mtail[m[35m -n +2)[m; [38;5;130mdo[m[41;7H[36mnam[m=[35m`stat -t -c %n ${datadir}/*${biosample}*${chr}_*.txt*`[m[42;7H[38;5;130mif [m[35m[[[m [38;5;130m-e[m [35m${nam}[m [35m]][m; [38;5;130mthen[m[43;9H[36mcmd[m=[38;5;130m"[m[35m$cmd[m[31m <(zcat -f -- [m[38;5;130m"[m[35m${nam}[m[38;5;130m"[m[31m)[m[38;5;130m"[m;[44;7H[38;5;130mfi
[m    [38;5;130mdone
[m    [38;5;130meval[m [35m$cmd[m | awk [38;5;130m'[m[31mBEGIN{OFS="\t";chr=""}
      {if (NR==1) { chr=$2 } else { if (NR>2) print chr, (NR-3)*200, (NR-2)*200, $0}}[m[38;5;130m'[m \[48;7H[38;5;130m>[m matrix_[35m${chr}[m.txt
  [38;5;130melse
[m    [38;5;130mecho[m[31m [m[38;5;130m"[m[31mno files found[m[38;5;130m"
[m  [38;5;130mfi
done[m[56;1H[94m~                                                                                                                                                                                                                   [57;1H~                                                                                                                                                                                                                   [m[58;195H44,7[10CAll[44;7H[?12l[?25h[?25l[58;196H3[43;7H[?12l[?25h[?25l[58;196H2[42;7H[?12l[?25h[?25l[58;198H8[42;8H[?12l[?25h[?25l[58;198H9[42;9H[?12l[?25h[?25l[1C[35m[106m[[13C][m[58;198H10[42;10H[?12l[?25h[?25l[35m[[m[35m[106m[[11C][m[35m][m[58;199H1[42;11H[?12l[?25h[?25l[35m[[11C][m[58;199H2[42;12H[?12l[?25h[?25l[58;199H3[42;13H[?12l[?25h[?25l[58;199H4[42;14H[?12l[?25h[?25l[58;199H5[42;15H[?12l[?25h[?25l[58;199H6[42;16H[?12l[?25h[?25l[1C[35m[106m{[3C}[m[58;199H7[42;17H[?12l[?25h[?25l[35m{nam}[m[58;199H8[42;18H[?12l[?25h[?25l[58;199H9[42;19H[?12l[?25h[?25l[58;198H20[42;20H[?12l[?25h[?25l[35m[106m{[3C}[m[58;199H1[42;21H[?12l[?25h[?25l[58;1H[1m-- INSERT --[m[58;13H[K[58;195H42,22[9CAll[58;195H[K[58;195H42,22[9CAll[42;22H[?12l[?25h[?25l[97m[106m [m[97m[101m]]; then[m[42;30H[K[43;1H[97m[101m        cmd="[4C <(zcat -f -- "[6C")";
      fi
    done
    eval [4C | awk 'BEGIN{[m[35mOFS[m[38;5;130m="[m[35m\t[m[38;5;130m"[m[97m[101m;chr=""[m[35m}[m
      [35m{[m[38;5;130mif [m[35m([m[36mNR[m==[1C[35m)[m [35m{[m [36mchr[m=[35m$2[m [35m}[m [38;5;130melse[m [35m{[m [38;5;130mif [m[35m([mNR>[1C[35m)[m [38;5;130mprint[m[6C[35m([mNR[2C[35m)[m*[3C, [35m([mNR[2C[35m)[m*[3C, [35m$0[m[97m[101m}}[m[1C[31m \
      > matrix_${chr}.txt
  else
    echo "[14C"
  fi
done[m[42;17H[35m{[m[3C[97m[101m [m[58;199H1[42;21H[?12l[?25h[?25l[97m[101m ]]; then[m[42;27H[K[58;195H[K[58;195H42,18[9CAll[42;18H[?12l[?25h[?25l$ [35m]][m; [38;5;130mthen[m[42;26H[K[43;1H        [36mcmd[m=[38;5;130m"[m[4C[31m <(zcat -f -- [m[38;5;130m"[6C"[m[31m)[m[38;5;130m"[m;
      [38;5;130mfi[m
    [38;5;130mdone[m
    [38;5;130meval[m [4C | awk [38;5;130m'[m[31mBEGIN{OFS="\t";chr=""}
      {if (NR==1) { chr=$2 } else { if (NR>2) print chr, (NR-3)*200, (NR-2)*200, $0}}[m[1C \
      [38;5;130m>[m matrix_[35m${chr}[m.txt
  [38;5;130melse[m
    [38;5;130mecho[1C"[14C"[m
  [38;5;130mfi
done[m[58;199H7[42;17H[?12l[?25h[?25l [35m][m[1C; [38;5;130mthen[m[42;25H[K[58;199H6[42;16H[?12l[?25h[?25l[38;5;130m"[m[31m ]]; then
        cmd=[m[5C [38;5;130m<[m(zcat [38;5;130m-f[m -- [8C)[1C[31m;
      fi
    done
    eval [4C | awk '[m[10C[38;5;130m"[m[35m\t[m[38;5;130m"[5C""[m[47;25H[35m$2[55C$0[m[2C[31m' \
      > matrix_[6C.txt
  else
    echo[m[2Cno files found
[31m  fi
done[m[58;199H7[42;17H[?12l[?25h[?25l[35m${datadir}[m[31m/*[m[35m${biosample}[m[31m*[m[35m${chr}[m[31m_*.txt* ]]; then[m[58;198H55[42;55H[?12l[?25h[?25l[38;5;130m"[m [35m]][m; [38;5;130mthen[m
        [36mcmd[m=[5C[31m <(zcat -f -- [8C)[m[1C;
      [38;5;130mfi[m
    [38;5;130mdone[m
    [38;5;130meval[m [4C | awk [38;5;130m'[m[10C[31m"\t";chr=""[47;25H$2[55C$0[m[2C[38;5;130m'[m \
      [38;5;130m>[m matrix_[6C.txt
  [38;5;130melse[m
    [38;5;130mecho[m[2C[31mno files found[m
  [38;5;130mfi
