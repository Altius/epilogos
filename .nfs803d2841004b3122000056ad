      {if (NR==1) { chr=$2 } else { if (NR>2) print chr, (NR-3)*200, (NR-2)*200, $0}}[m[38;5;130m'[m \[46;7H[38;5;130m>[m matrix_[35m${chr}[m.txt
  [38;5;130melse
