[m    [38;5;130mfor[m biosample [38;5;130min[m [35m$(cut -f1 ${metadata} | [m[38;5;130mtail[m[35m -n +2)[m; [38;5;130mdo[m[41;7H[36mnam[m=[35m`stat -t -c %n ${datadir}/*${biosample}*${chr}_*.txt*`[m[42;7H[38;5;130mif [m[35m[[[m [38;5;130m-e[m [38;5;130m"[m[35m${datadir}[m[31m/*[m[35m${biosample}[m[31m*[m[35m${chr}[m[31m_*.txt*[m[38;5;130m"[m [35m]][m; [38;5;130mthen[m[43;9H[36mcmd[m=[38;5;130m"[m[35m$cmd[m[31m <(zcat -f -- [m[38;5;130m"[m[35m${nam}[m[38;5;130m"[m[31m)[m[38;5;130m"[m;[44;7H[38;5;130mfi
