  [38;5;130mecho[m[31m [m[35m$numfiles[m
  [38;5;130mif [[m [35m${numfiles}[m [38;5;130m-gt[m [31m0[m [38;5;130m][m; [38;5;130mthen
[m    [38;5;130mecho[m[31m -n [m[38;5;130m"[m[35m${numfiles}[m[31m potential files, [m[38;5;130m"[m
    [36mi[m=[31m0[m;
    [38;5;130mfor[m biosample [38;5;130min[m [35m$(cut -f1 ${metadata} | [m[38;5;130mtail[m[35m -n +2)[m; [38;5;130mdo[43;7Hif compgen[m [38;5;130m-G[m [38;5;130m"[m[35m${datadir}[m[31m/*[m[35m${biosample}[m[31m*[m[35m${chr}[m[31m_*.txt*[m[38;5;130m"[m [38;5;130m>[m /dev/null; [38;5;130mthen[m[44;9H[36mnam[m=[35m`stat -t -c %n ${datadir}/*${biosample}*${chr}_*.txt*`[m[45;9H[36mcmd[m=[38;5;130m"[m[35m$cmd[m[31m <(zcat -f -- [m[38;5;130m"[m[35m${nam}[m[38;5;130m"[m[31m)[m[38;5;130m"[m;[46;9H[36mi[m=[35m$((i+[m[31m1[m[35m))[m[47;7H[38;5;130mfi
