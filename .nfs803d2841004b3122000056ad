[m    [38;5;130mecho[m[31m [m[38;5;130m"[m[35m${numfiles}[m[31m files[m[38;5;130m"
[m    [38;5;130mfor[m biosample [38;5;130min[m [35m$(cut -f1 ${metadata} | [m[38;5;130mtail[m[35m -n +2)[m; [38;5;130mdo[41;7Hif [m[35m[[[m [38;5;130m-f[m [38;5;130m"[m[35m${datadir}[m[31m/*[m[35m${biosample}[m[31m*[m[35m${chr}[m[31m_*.txt*[m[38;5;130m"[m [35m]][m; [38;5;130mthen[m[42;9H[36mnam[m=[35m`stat -t -c %n ${datadir}/*${biosample}*${chr}_*.txt*`[m[43;9H[38;5;130mecho[m[31m [m[35m$nam[m[44;9H[36mcmd[m=[38;5;130m"[m[35m$cmd[m[31m <(zcat -f -- [m[38;5;130m"[m[35m${nam}[m[38;5;130m"[m[31m)[m[38;5;130m"[m;[45;7H[38;5;130mfi
[m    [38;5;130mdone
[m    [38;5;130meval[m [35m$cmd[m | awk [38;5;130m'[m[31mBEGIN{OFS="\t";chr=""}
      {if (NR==1) { chr=$2 } else { if (NR>2) print chr, (NR-3)*200, (NR-2)*200, $0}}[m[38;5;130m'[m \[49;7H[38;5;130m>[m matrix_[35m${chr}[m.txt
  [38;5;130melse
