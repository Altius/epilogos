[?1049h[?1h=[2;1H▽[6n[1;1H  [1;1H[1;58r[?12;25h[?12l[?25h[27m[m[H[2J[?25l[58;1H"scripts/preprocess_data_ChromHMM.sh" 54L, 2132C[>c[1;1H[34m#!/bin/bash[m
[38;5;130mset[m[36m [m[35m-ueo[m[36m pipefail[m

[38;5;130mif [[m [35m$#[m [38;5;130m-lt[m [31m3[m [38;5;130m][m; [38;5;130mthen
[m  [38;5;130mecho
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31mUsage: [m[35m$0[m[31m <chromsizes> <metadata> <datadir>[m[38;5;130m"
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31mInsufficient information -- please provide:[m[38;5;130m"
[m  [38;5;130mecho
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31m1. <datadir>: Directory with input data, one file per biosample-chromosome combination.[m[38;5;130m"
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31m   These can be obtained from ChromHMM using the '-printstatebyline' parameter.[m[38;5;130m"
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31m   Each line expected to represent a single 200bp genomic bin and its chromatin state.[m[38;5;130m"
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31m   For more information on the formatting of these files, see the ChromHMM manual at:[m[38;5;130m"
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31m     http://compbio.mit.edu/ChromHMM/ChromHMM_manual.pdf[m[38;5;130m"
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31m   Example files can be found in the 'data/ChromHMM' directory.[m[38;5;130m"
[m  [38;5;130mecho
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31m2. <metadata>: File containing metadata information for each input dataset/biosample.[m[38;5;130m"
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31m   This file is expected to contain a header line, and a unique biosample name in column 1,[m[38;5;130m"
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31m   represented in the input data directory. Example file: 'data/metadata_Boix.txt'.[m[38;5;130m"
[m  [38;5;130mecho
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31m3. <chromsizes>: File containing chromosome sizes in number of basepairs.[m[38;5;130m"
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31m   This information can be obtained from UCSC, e.g. for human genome hg19, run:[m[38;5;130m"
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31m   $ wget -q -O - ftp://hgdownload.cse.ucsc.edu/goldenPath/hg19/database/chromInfo.txt.gz | [m[35m\\[m[38;5;130m"
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31m       gunzip - | cut -f 1-2 > hg19.genome[m[38;5;130m"
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31m   Examples for hg19 and hg38 can be found in the 'scripts' directory.[m[38;5;130m"
[m  [38;5;130mecho
[m  [38;5;130mecho[m[31m [m[38;5;130m"[m[31mExample run: [m[35m$0[m[31m data/ChromHMM data/metadata_Boix.txt scripts/hg19.genome[m[38;5;130m"
[m  [38;5;130mexit
fi[m

[36mdatadir[m=[35m${1}[m
[36mmetadata[m=[35m${2}[m
[36mchromsizes[m=[35m${3}[m

[38;5;130mfor[m chr [38;5;130min[m [35m$(cut -f1 ${chromsizes})[m; [38;5;130mdo
[m  [38;5;130mecho[m[31m -n [m[38;5;130m"[m[31mProcessing [m[35m${chr}[m[31m: [m[38;5;130m"[m
  [36mcmd[m=[38;5;130m"[m[31mpaste[m[38;5;130m"[m;
  [36mnumfiles[m=[35m`[m[38;5;130mls[m[35m [m[31m-1[m[35m ${datadir}/*${chr}_*.txt* [m[31m2[m[35m>/dev/null | wc -l`[m
  [38;5;130mif [[m [35m${numfiles}[m [38;5;130m-gt[m [31m0[m [38;5;130m][m;
  [38;5;130mthen
[m    [38;5;130mecho[m[31m [m[38;5;130m"[m[35m${numfiles}[m[31m files[m[38;5;130m"
[m  [38;5;130mfor[m biosample [38;5;130min[m [38;5;130m"[m[35m$(cut -f1 ${metadata} | [m[38;5;130mtail[m[35m -n +2)[m[38;5;130m"[m; [38;5;130mdo[m
    [36mnam[m=[35m`stat -t -c %n [m[38;5;130m"[m[35m${datadir}[m[31m/*[m[35m${biosample}[m[31m*[m[35m${chr}[m[31m_*.txt*[m[38;5;130m"[m[35m`[m
    [36mcmd[m=[38;5;130m"[m[35m$cmd[m[31m <(zcat -f -- [m[35m${nam}[m[31m)[m[38;5;130m"[m;
  [38;5;130mdone
[m  [38;5;130meval[m [35m$cmd[m | awk [38;5;130m'[m[31mBEGIN{OFS="\t";chr=""}
    {if (NR==1) { chr=$2 } else { if (NR>2) print chr, (NR-3)*200, (NR-2)*200, $0}}[m[38;5;130m'[m \
    [38;5;130m>[m matrix_[35m${chr}[m.txt
  [38;5;130melse
